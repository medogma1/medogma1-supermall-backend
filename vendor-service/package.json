{
  "name": "vendor-service",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node --no-deprecation index.js",
    "dev": "nodemon --no-deprecation index.js",
    "test": "mocha tests/**/*.test.js",
    "test:unit": "mocha tests/controllers.test.js tests/models.test.js",
    "test:api": "mocha tests/test-create-vendor-supertest.js",
    "test:jest": "jest --config jest.config.js",
    "test:cypress": "cypress run",
    "test:postman": "newman run tests/postman/vendor-api.postman_collection.json -e tests/postman/vendor-api.postman_environment.json",
    "test:all": "node tests/run-all-tests.js",
    "cypress:open": "cypress open",
    "coverage": "nyc --reporter=html --reporter=text npm test"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "mysql2": "^3.6.5",
    "slugify": "^1.6.6",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "axios": "^0.27.2",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.2",
    "chai-http": "^4.4.0",
    "cypress": "^10.3.0",
    "jest": "^28.1.3",
    "jest-html-reporter": "^3.6.0",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "newman": "^5.3.2",
    "newman-reporter-htmlextra": "^1.22.11",
    "nodemon": "^3.0.2",
    "nyc": "^15.1.0",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.2.4"
  }
}
